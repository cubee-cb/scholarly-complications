function _init()
 loadconstants()
 fadet=0

 wob={}
 
 -- load wobblepaints
 local src,i=0x1000,0
 while %src>0 do
	i+=1
	wob[i]=wob_load(src)
	src+=%src
 end

 fade()
 fadet=0

end

cartdata"cubg-nara"

function _update()
if(debug)menuitem(4,"skip",function()level+=1 init_level(level,true) end)

reload()

-- compensate for 30fps
for lp=0,_update and 1 or 0 do
btnp=lp==1 and function()return end or _btnp

if(freezeframe>0)freezeframe-=1 return

if mode=="splash" then
 if t>150 or btnp(4) then
  if(btnp(4))fade()
  mode="title"
  sfx(-1)

  -- copy actor gfx
  memcpy(0,0x8000,0x2000)
  fadet=1
 end

 -- play pxlshk sound
 if t==32 then
  sfx(63)
 end

elseif mode=="title" then

 if(btnp(2))plrs-=1 ssfx(14,0)
 if(btnp(3))plrs+=1 ssfx(14,0)
 plrs%=maxplayers

 if btnp(4) then
  ssfx(14,1)
  fade()

  charsel,charconf,mode,countdown
  ={},{},spread"charsel,0"

  for i=0,plrs do
   add(charsel,i)
   add(charconf,false)
  end
 end

elseif mode=="charsel" then

 for i=0,plrs do
  local k=i+1
  if not charconf[k] then
	  if(btnp(0,i))charsel[k]-=1 ssfx(14,0)
	  if(btnp(1,i))charsel[k]+=1 ssfx(14,0)

	  charsel[k]%=#characters
  end

  -- confirm/cancel selection

	 local conf=true
	 for i in all(charconf) do
	  if(i==charsel[k])conf=false
	 end

  if btnp(4,i) then
   if conf then
    ssfx(14,1)
    charconf[k]=charsel[k]
   else
    ssfx(14,3)
   end
  end
 
  if btnp(5,i) then
   ssfx(14,2)
   if charconf[k] then
    charconf[k]=false
   else
    fade()
    mode="title"
   end
  end
 end

 local conf=true
 for i in all(charconf) do
  if(not i)conf=false
 end

 if conf then
  countdown-=1
  if countdown<=0 then
	  ssfx(14,4)
	  fade()

   local str=""
   for k,i in pairs(charsel) do
    str..=characters[(i+1)]
    if(k<#charsel)str..=","
   end

   -- load game cart
   dset(63,1)
   --load("beatem-packed","no go back aaaaaaaaa",str)
   load("beatem-cutscenes","no go back aaaaaaaaa",str)

	  --music(0)
	  
	 elseif countdown%30==0 then
	  ssfx(14,5)
  end
 else
  countdown=plrs==0 and 0 or 120
 end

elseif mode=="fail" then
 if btnp(4) then
  fade()
  run()
 end

else
 mode="splash"
end

t=max(t+1)

end

end




